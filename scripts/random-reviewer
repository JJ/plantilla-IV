  package LWP::MemberMixin;
  
  our $VERSION = '6.67';
  
  sub _elem {
      my $self = shift;
      my $elem = shift;
      my $old  = $self->{$elem};
      $self->{$elem} = shift if @_;
      return $old;
  }
  
